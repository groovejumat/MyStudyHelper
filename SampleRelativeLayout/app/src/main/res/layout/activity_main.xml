<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="함께 공부하기"
        android:textColor="#000000"
        android:textColorLink="#040404"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView26"
        app:layout_constraintEnd_toEndOf="@+id/signup"
        app:layout_constraintHorizontal_bias="0.38"
        app:layout_constraintStart_toStartOf="@+id/signup"
        app:layout_constraintTop_toTopOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="메모하기"
        android:textColor="#000000"
        android:textColorLink="#040404"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/memo"
        app:layout_constraintHorizontal_bias="0.539"
        app:layout_constraintStart_toStartOf="@+id/memo"
        app:layout_constraintTop_toBottomOf="@+id/memo"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/whitenoisebt"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="138dp"
        android:layout_height="127dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/soundicon"
        android:backgroundTint="#080000"
        android:hapticFeedbackEnabled="true"
        android:textColorLink="#00FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/concentratebt"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/concentratebt"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/appusage"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/calendar"
        android:backgroundTint="#030000"
        android:hapticFeedbackEnabled="true"
        android:textColorLink="#00FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/post"
        app:layout_constraintStart_toStartOf="@+id/post"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="스케쥴 작성하기"
        android:textColor="#000000"
        android:textColorLink="#040404"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/appusage"
        app:layout_constraintStart_toStartOf="@+id/appusage"
        app:layout_constraintTop_toBottomOf="@+id/appusage" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="백색소음틀기"
        android:textColor="#000000"
        android:textColorLink="#040404"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/whitenoisebt"
        app:layout_constraintStart_toStartOf="@+id/whitenoisebt"
        app:layout_constraintTop_toBottomOf="@+id/whitenoisebt" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="클립보드"
        android:textColor="#000000"
        android:textColorLink="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/post"
        app:layout_constraintStart_toStartOf="@+id/post"
        app:layout_constraintTop_toBottomOf="@+id/post" />

    <Button
        android:id="@+id/post"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="138dp"
        android:layout_height="127dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/memo"
        android:hapticFeedbackEnabled="true"
        android:textColorLink="#00FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/concentratebt"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/concentratebt"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="138dp"
        android:layout_height="127dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/focus"
        android:hapticFeedbackEnabled="true"
        android:textColorLink="#00FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/post"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/post" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="집중시작하기"
        android:textColor="#000000"
        android:textColorLink="#040404"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/concentratebt"
        app:layout_constraintStart_toStartOf="@+id/concentratebt"
        app:layout_constraintTop_toBottomOf="@+id/concentratebt" />

    <Button
        android:id="@+id/memo"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="138dp"
        android:layout_height="127dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/note"
        app:layout_constraintEnd_toEndOf="@+id/whitenoisebt"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/whitenoisebt"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/signup"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="138dp"
        android:layout_height="127dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/people"
        app:layout_constraintEnd_toEndOf="@+id/appusage"
        app:layout_constraintStart_toStartOf="@+id/appusage"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/studychecker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>